[paths]
semgrep_rules_dir = ""
clone_base_dir = ""

[settings]
exclude_langs = ["Dockerfile", "Makefile", "YAML", "JSON", "HTML", "Terraform"]
suppress_paths = ["best-practice", "correctness", "portability", "maintainability"]
suppress_rules = [
    "go.lang.security.audit.crypto.missing-ssl-minversion",
    "go.lang.security.audit.use-of-unsafe-block",
    "go.lang.security.audit.crypto.math-random-used"
]
gemini_model = "gemini-2.5-flash-lite"

[mongo]
mongo_path = "localhost:27017"

[tokens]
github_token = ""
gemini_api_key ""

[deployment]
host = "127.0.0.1"
port = 9001